@model Bdproductos.Models.User

<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="detallesProductoModalLabel">Detalles del Usuario</h5>
           <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" onclick="window.location.href='/User'"></button>

        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-md-8">
                    <p><strong>Id:</strong> @Model.Id</p>
                    <p><strong>Names:</strong> @Model.Names</p>
                    <p><strong>LastNames:</strong> @Model.LastNames</p>
                    <p><strong>Email:</strong> @Model.Email</p>
                    <p><strong>Phone:</strong> @Model.Phone</p>
                    <p><strong>Address:</strong> @Model.Address</p>
                </div>
            </div>
        </div>
    </div>
</div>
